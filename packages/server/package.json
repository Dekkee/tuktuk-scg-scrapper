    "async": "^3.1.0",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "flexsearch": "^0.6.30",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "node-fetch": "^2.6.0",
    "query-string": "^6.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
